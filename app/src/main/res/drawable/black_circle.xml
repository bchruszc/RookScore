            <corners android:radius="4dp" />

            <solid android:color="#E6E6E6" />
        </shape>
    </item>
    <item
        android:bottom="29dp"
        android:left="10dp"
        android:right="10dp"
        android:top="29dp">
        <shape android:shape="rectangle" >
            <corners android:radius="4dp" />

            <solid android:color="#E6E6E6" />
        </shape>
    </item>

</layer-list>